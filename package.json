{
  "name": "dragon-city-helper",
  "version": "1.6.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "prisma:db:push": "dotenv -e .env.local prisma db push",
    "prisma:seed": "dotenv -e .env.local node prisma/seed.mjs",
    "prisma:seed:other": "dotenv -e .env.local node prisma/seed-ditlep-dc.mjs",
    "prisma:update:types": "dotenv -e .env.local node prisma/updateTypes.mjs",
    "revalidate": "dotenv -e .env.local node scripts/revalidate.mjs",
    "pull:master:data": "dotenv -e .env.local node scripts/pullMaster.mjs",
    "changeset": "changeset",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@mantine/core": "^7.16.3",
    "@mantine/form": "^7.17.7",
    "@mantine/hooks": "^7.16.3",
    "@mantine/notifications": "^7.17.1",
    "@next/third-parties": "^14.2.18",
    "@prisma/client": "^6.8.2",
    "@sentry/nextjs": "^9.1.0",
    "@tabler/icons-react": "^3.30.0",
    "date-fns": "^4.1.0",
    "next": "^14.2.30",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.12",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/react": "^18",
    "@vercel/blob": "^0.27.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss-preset-mantine": "^1.17.0",
    "jest-environment-jsdom": "^29.7.0",
    "prisma": "^6.8.2",
    "tailwindcss": "^3.4.9",
    "typescript": "^5.7.3"
  }
}
