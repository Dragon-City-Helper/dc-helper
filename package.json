{
  "name": "dragon-city-helper",
  "version": "1.2.1",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --passWithNoTests",
    "prisma:db:push": "dotenv -e .env.local prisma db push",
    "prisma:seed": "dotenv -e .env.local node prisma/seed.mjs",
    "revalidate": "dotenv -e .env.local node scripts/revalidate.mjs",
    "pull:master:data": "dotenv -e .env.local node scripts/pullMaster.mjs",
    "changeset": "changeset",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "jest --bail --findRelatedTests"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.3",
    "@mantine/core": "^7.14.3",
    "@mantine/form": "^7.14.3",
    "@mantine/hooks": "^7.14.3",
    "@next/third-parties": "^14.2.18",
    "@prisma/client": "^6.0.0",
    "@sentry/nextjs": "^8.49.0",
    "@tabler/icons-react": "^3.21.0",
    "next": "^14.2.18",
    "next-auth": "^5.0.0-beta.25",
    "react": "^18",
    "react-dom": "^18",
    "react-intersection-observer": "^9.13.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vercel/blob": "^0.26.0",
    "autoprefixer": "^10.4.20",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.47",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "prisma": "^6.0.0",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  }
}
